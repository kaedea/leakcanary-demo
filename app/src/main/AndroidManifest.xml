<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="me.kaede.leakcanarydemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:name=".ExampleApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".localvariable.LocalVariableActivity"
                android:label="@string/app_name">
        </activity>
        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activityleak.ActivityLeakActivity">
        </activity>
        <activity android:name=".fragmentleak.FragmentLeakActivity">
        </activity>
        <activity android:name=".singleton.SingletonActivity">
        </activity>
    </application>

</manifest>
